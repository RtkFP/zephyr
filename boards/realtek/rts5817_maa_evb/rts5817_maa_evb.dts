/dts-v1/;
#include <realtek/fingerprint/rts5817/rts5817.dtsi>
#include "rts5817_maa_evb-pinctrl.dtsi"

/ {
	model = "Realtek rts5817_maa_evb";
	compatible = "realtek,rts5817_maa_evb";
	#address-cells = <1>;
	#size-cells = <1>;

	chosen {
		zephyr,console = &uart0;
		zephyr,shell-uart = &uart0;
	};
};

&clks {
	status = "okay";
};

&reset {
	status = "okay";
};

&watchdog {
	status = "okay";
};

&uart0 {
	pinctrl-names = "default";
	pinctrl-0 = <&uart0_default>;
	current-speed = <115200>;
	status = "okay";
};

&uart1 {
	status = "disabled";
};

&gpio {
	status = "okay";
};

&quadspi {
	status = "okay";
};

&regulator {
	status = "okay";
	svio {
		regulator-always-on;
	};
	sva {
		regulator-always-on;
	};
};

&spi0 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&sensor_spi_default>;
};

&spi1 {
	status = "okay";
	pinctrl-names = "default";
	pinctrl-0 = <&ssi_master_default>;
};

zephyr_udc0: &usb {
	status = "okay";
};
